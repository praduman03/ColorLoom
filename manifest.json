{
  "name": "ColorLoom",
  "description": "Discover website colors effortlessly with precision.",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
    "default_popup": "./Popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./contentScript.js"]
    }
  ],
  "icons": {
    "128": "logo.png"
  },
  "permissions": ["tabs", "storage"],
  "host_permissions": ["<all_urls>"]
}
